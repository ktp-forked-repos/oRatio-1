            goal going = new GoingTo_b_a(end:start);
        } or {
            goal going = new GoingTo_c_a(end:start);
        } or {
            goal going = new GoingTo_e_a(end:start);
        } or {
            goal going = new GoingTo_f_a(end:start);
        } or {
            goal going = new GoingTo_j_a(end:start);
        }
    }

    predicate GoingTo_b_a() { duration == 100.0; goal at = new At_b(end:start); }
    predicate GoingTo_c_a() { duration == 200.0; goal at = new At_c(end:start); }
    predicate GoingTo_e_a() { duration == 100.0; goal at = new At_e(end:start); }
