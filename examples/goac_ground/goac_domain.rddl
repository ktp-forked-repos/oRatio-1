class Visibility : StateVariable {

    predicate Visible() {
        false;
    }

    predicate NotVisible() {
        false;
    }
}

class Robot {

    Navigator navigator = new Navigator();
    ReusableResource antenna = new ReusableResource(1.0);
    Visibility visibility;

    Robot(Visibility visibility) : visibility(visibility) {}

    predicate At_a() : IntervalPredicate { goal at = new navigator.At_a(start:start, end:end, duration:duration); }
    predicate At_b() : IntervalPredicate { goal at = new navigator.At_b(start:start, end:end, duration:duration); }
