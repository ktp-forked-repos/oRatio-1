    </Expand>
  </Type>

  <Type Name="ac::enum_eq_propagator&lt;*&gt;">
    <DisplayString>[{_eq,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::geq_propagator">
