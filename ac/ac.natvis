    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::sum_propagator">
    <DisplayString>[{_sum,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::product_propagator">
    <DisplayString>[{_prod,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::div_propagator">
    <DisplayString>[{_div,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::lt_propagator">
    <DisplayString>[{_lt,na}]</DisplayString>
