<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  <Type Name="ac::var">
    <DisplayString>{_name} = {_domain,na}</DisplayString>
    <Expand>
      <ExpandedItem>_domain,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::enum_domain&lt;*&gt;">
    <DisplayString>{_vals}</DisplayString>
    <Expand>
      <ExpandedItem>_vals,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::interval">
    <DisplayString Condition="lb == ub">{lb}</DisplayString>
    <DisplayString>[{lb}, {ub}]</DisplayString>
  </Type>

  <Type Name="ac::arith_domain">
    <DisplayString>{_interval}</DisplayString>
    <Expand>
      <ExpandedItem>_interval,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::not_propagator">
    <DisplayString>[{_not,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::and_propagator">
    <DisplayString>[{_and,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::or_propagator">
    <DisplayString>[{_or,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::exct_one_propagator">
    <DisplayString>[{_exct_one,na}]</DisplayString>
    <Expand>
      <ExpandedItem>_vars,na</ExpandedItem>
    </Expand>
  </Type>

  <Type Name="ac::minus_propagator">
